            text-transform: none;
            font-weight: 500;
            line-height: -10px;
        }

        /*
            ADMINISTRATION V2
        */
        
        .administration___container {
            display: inline-block;
            position: relative;
            padding:0 10px;
            width: 20cm;
            height: 23cm;
            margin: 0 auto;
            margin-top: 120px;
            border:1px solid transparent;
            text-shadow: 0px 1px 1px #808080;
            text-align: center;
            word-break: break-all;
        }

        .administration___container > .administration___box {
            position: relative;
            display: inline-block;
            margin: 10px;
            max-width: 221px;
            width: 100%;
            padding: 10px;
            text-align: center;
            vertical-align: top;
        }

        .administration___box > img{
            border: 2px solid red;
            background-color: white;
            text-align: center;
            width: 100px;
            height: 100px;
            box-shadow: 5px 5px 5px gray;
            object-fit: cover;
        }

        .administration___box > p {
            font-weight: 600;
            font-size: 13px;
            margin: 0;
        }

        .administration___box > span {
            display:block;
            font-weight: normal;
            font-size: 12px;
        }

        /* ============================================================================================================ */

        /*
            GALLERY
        */
        
        :root {
            --dark: #373349;
            --shadow: rgba(0,0,0,.2);
        }

        .gallery__container > a {
            display: flex;
            justify-content: center;
            align-items: center;
            font-size: 2em;
        }

        .gallery__container > a > img {
            width: 100%;
            height: 100%;
            object-fit: cover;
            box-shadow: 0 2px 16px var(--shadow);
        }

        .gallery__container {
            overflow: hidden;
            display: grid;
            grid-gap: 10px;
            grid-template-columns: repeat(auto-fit, minmax(150px, 1fr));
            grid-auto-rows: 120px;
            grid-auto-flow: dense;

            width: 20cm;
            height: 28.7cm;
            border: 1px solid red;
            padding-top: 20px;
        }

        .horizontal {
            grid-column: span 1;
        }

        .vertical {
            grid-row: span 2;
        }

        .big {
            grid-column: span 2;
            grid-row: span 2;
        }

        /* ============================================================================================================ */

        @page {
            size: A4;
            margin: 0;
        }
        @media print {
            .page {
                margin: 0;
                border: initial;
                border-radius: initial;
                width: initial;
                min-height: initial;
                box-shadow: initial;
                background: initial;
                page-break-after: always;
            }
        }

        .back__container {
            column-count: 2;
            column-fill:auto;
            display: inline-block;
            position: relative;
            padding:0 10px;
