        }

        .alumni__display.display3 > .nametag__container > span{
            display: block;
            position: absolute;
            font-size: 11px;
            width: 100%;
            {{!-- bottom: 10px; --}}
        }

        .alumni___container > .alumni__display.display4{
            position: relative;
