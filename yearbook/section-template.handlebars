        body {
            margin: 0;
            padding: 0;
            background-color: #FAFAFA;
            font-family: 'Lora', serif;
        }

        html{
            -webkit-print-color-adjust:exact;
            -webkit-filter: opacity(1) !important;
            font-family: 'Lora', serif;
        }
        
        * {
            box-sizing: border-box;
            -moz-box-sizing: border-box;
        }
        .page {
            width: 21cm;
            min-height: 29.713cm;
            padding: 0; /*2cm*/
            margin: auto;
            border: 1px #D3D3D3 solid;
            border-radius: 5px;
            background: white;
            box-shadow: 0 0 5px rgba(0, 0, 0, 0.1);
        }

        .page.cover{
            background: url('http://localhost:3001/cover.jpg');
            background-repeat: no-repeat;
            background-size: cover; 
        }

        .page.commence{
            position: relative;
            background: url('http://localhost:3001/cover.jpg');
            background-repeat: no-repeat;
            background-size: cover; 
        }

        .page.inst{
            position: relative;
            background: url('http://localhost:3001/cover.jpg');
            background-repeat: no-repeat;
            background-size: cover; 
        }

        .page.alumni{
            background-repeat: no-repeat;
            background-size: cover; 
        }

        .subpage {
            position: relative;
            padding: 10px;
            border: 5px red transparent;
            height: 296.5mm;
            outline: 0 #FFEAEA solid;
        }
        /* ============================================================================================================ */

        /*
            MISION, VISION, GOALS, CORE VALUES
        */
        
        .header__main{
            width:100%;
        }

        .header__main img{
            position:absolute;
            float:left;
            width:125px;
            height:125px;
            z-index: 99;
        }
    
        .header__main .title__, .header__main .title__sub{
            float:left;
            height:1.5cm;
            width: 19cm;
            position:absolute;
            margin:35px 50px;
            font-size: 40px;
            /* margin-left: 2.7cm; */
            text-align: center;
            line-height: 50px;
            font-weight: 600;
            text-transform:uppercase;
            text-shadow: 1px 2px 1px gray;
        }

        .header__main .title__sub{
            font-family: 'Bangers';
            padding-top: 40px;
            padding-left: 150px;
            font-size: 20px;
            line-height: 50px;
            font-weight: 500;
            text-transform:none;
        }

        .mvgc___container {
            font-family: 'Lora', serif;

            position: relative;
            width: 20cm;
            height: 28.7cm;
            margin-top: 10px;
            border:1px solid transparent;
            overflow: hidden;
            text-align: center;
            padding-top: 5.3cm;
            text-shadow: 0px 1px 1px #808080;
        }

        .mvgc___container > .mvgc___box {
            width: 15cm;
            border:1px solid transparent;
            margin: auto;
            margin-bottom: 10px;
        }

        .mvgc___container .mvgc___box > label {
            font-size: 30px;
            text-transform:uppercase;
            font-weight: 600;
        }

        .mvgc___container .mvgc___box > p {
            
            font-size: 15px;
            text-transform:uppercase;
            font-weight: 600;
            white-space: pre-wrap;
        }

        /* ============================================================================================================ */
        
        /*
            HISTORY
        */

        .history___container {
            width: 20cm;
            height: 28.7cm;
            margin-top: 10px;
            border:1px solid transparent;
            overflow: hidden;
        }
        
        .history___container img {
            float: left;
            width: 350px;
            margin: 35px 10px;
            border: 5px solid white;
        }

        .history___container p {
            text-align: justify;
            text-justify: inter-word;
            font-size: 13.5px;
            margin: 10px;
            text-shadow: 0px 1px 1px #808080;
            white-space: pre-line;
        }
        /* ============================================================================================================ */

        /*
            COMMENCE
        */

        .header .title__, .header .title__sub{
            float:left;
            height:1.5cm;
            width: 19cm;
            position:absolute;
            margin:35px 35px;
            font-size: 55px;
            /* margin-left: 2.7cm; */
            text-align: center;
            line-height: 50px;
            font-weight: 600;
            text-transform:uppercase;
            text-shadow: 1px 2px 1px gray;
        }

        .header .title__sub{
            margin:80px 35px;
            font-size: 30px;
            line-height: 50px;
            font-weight: 500;
        }

        .hr{
            position: absolute;
            left:0;
            right:0;

            margin-left: auto;
            margin-right: auto;

            height: 2px;
            width: 19cm;
            top:140px;
            z-index: 9999;
        }

        .commence___container {
            width: 20cm;
            height: 24.7cm;
            margin-top: 120px;
            border:1px solid transparent;
            word-break:keep-all;
            overflow: hidden;
        }

        .commence___container > img {
            float:left;
            width: 7cm;
            height: 8.7cm;
            margin: 70px 20px 40px 20px;
            border: 5px solid #fff;
            box-shadow: 5px 5px 2px #696969;
        }

        .commence___container > .content {
            text-align: justify;
            text-justify: inter-word;
            font-size: 14px;
            margin-left: 30px;
            margin-right: 30px;
            margin-bottom: 0;
            text-shadow: 0px 1px 1px #808080;
            white-space: pre-line;
        }

        .commence___container .quotes {
            text-align: center;
            font-size: 17px;
            margin-top: 100px;
            margin-left: 30px;
            margin-right: 30px;
            margin-bottom: 0;
            text-shadow: 0px 1px 1px #808080;
            white-space: pre-line;
            font-weight: 600;
        }

        .commence___container .signature__{
            position: relative;
