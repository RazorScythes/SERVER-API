            margin:auto;
            width:350px;
        }

        .institute div p{
            margin:0;
            font-size: 40px;
            text-align: center;
            font-weight: 700;
        }

        /*
            ALUMNI
        */
        .alumni___container{
            width: 20cm;
            height: 24.7cm;
            margin-top: 150px;
            margin-left:8px;
