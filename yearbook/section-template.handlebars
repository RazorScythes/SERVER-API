            padding-right: 10px;
            text-align: justify;
        }

        .back__three-parts h4{
            text-align: center;
            text-decoration: underline;
        }

        .back__prayer {
            max-height:33%;
            font-size: 12px;
            white-space: pre-line;
        }

        .back__oath {  
            max-height:33%;
            font-size: 12px;
            white-space: pre-line;
        }

        .back__graduation-song {
            max-height:33%;
            font-size: 12px;
           
        }

        .back__graduation-song > p {
            white-space: pre-line;
        }

        .back__closing-song {
            height: 100%;
            padding-left: 20px;
            padding-right: 20px;
            padding-top: 10px;
            overflow: hidden;
        }

        .back__closing-song p {
            white-space: pre-line;
            font-size: 14px;
            text-align: left;
        }

        .back__closing-song h4, .back__closing-song h5,  .back__graduation-song h4, .back__graduation-song h5, .back__graduation-song h6{
            text-align: center;
        }
    </style>
</head>
