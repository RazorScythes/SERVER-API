                                {{else}}
                                <label style="bottom:  calc({{../../../../nametag_props.name}}px - calc({{../../../../nametag_props.name}}px / 3))">{{this.name}}</label>
                            {{/if}}
                            {{#if ../../../../nametag_props.checked}} <span style="bottom:  {{../../../../nametag_props.quotes}}px" >{{#if this.quotes}}"{{this.quotes}}"{{/if}}</span>{{/if}}
                        </div>
                    </div>
                    
                    
                    {{/each}} 
                </div>                                     
            </div>    
        </div>
        {{/each}} 

        {{/each}}
    {{/each}}

