                    <label> The listing of names in this program does not serve as proof of graduation. This should be supported by the corresponding diploma and transcript of records</label>
                </div>
            </div>
		</div>    
    </div>
    {{/each}}

    {{#each objects}} 
        <div class="page inst"
            style=" background-image:url('{{this.background}}'); 
                    background-repeat: no-repeat;
                    background-size: cover; ">

            <div class="subpage">
                <div class="institute"
                    style="color: {{../main_header_color}}">
                    <div> <p>Batch {{this.batch}}</p> </div>
                    <img src="{{this.image}}"/>
                    <div> <p>{{this.institute}}</p> </div>
                </div>
            </div>    
        </div>
        {{#each graduates}}

        {{#each this.candidates}}
        <div class="page alumni" 
            style=" {{#if ../../institute_background}} background-image:url('{{../../institute_background}} {{else}} background-image:url('{{../../this.background}} {{/if}}'); 
                    background-repeat: no-repeat;
                    background-size: cover; ">
   
            <div class="subpage">
                                
                <div class="header"> 
                    <div> <p>{{../this.program}}</p> </div>
                    <img class="mcc_logo" src="{{../../this.logo}}"/>
                    <img class="institute_logo" src="{{../../this.image}}"/>
                </div>
                <div class="alumni___container">
            
           
                    {{#each this}}
                    {{#if ../../../../this.display_count}}
                            <div class="alumni__display display4">
                            {{else}}
                            <div class="alumni__display display3">
                    {{/if}}
                        {{#if this.sub}}
                            <img style="margin-right: 30px" src="{{this.main}}" class="main__image"/>
                            {{else}}
                            <img src="{{this.main}}" class="main__image"/>
                        {{/if}}
                        {{#if this.sub}}
                            <img src="{{this.sub}}" class="sub__image"/>
                        {{/if}}
                        <div class="nametag__container"
